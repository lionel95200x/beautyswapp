{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "monorepo": {
      "node": "20.18.2",
      "pnpm": "10.30.0",
      "env": {
        "EXPO_USE_FAST_RESOLVER": "true"
      }
    },
    "development": {
      "extends": "monorepo",
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development"
    },
    "preview": {
      "extends": "monorepo",
      "distribution": "internal",
      "environment": "preview"
    },
    "production": {
      "extends": "monorepo",
      "autoIncrement": true,
      "environment": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
