# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Payload CMS API Configuration
EXPO_PUBLIC_PAYLOAD_API_URL=http://localhost:3000

# Stripe Configuration
# Set to "true" to enable Stripe (requires build), "false" for dev with Expo Go
EXPO_PUBLIC_STRIPE_ENABLED=false
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# LogRocket Configuration
# Set to "true" to enable LogRocket session recording, "false" to disable
EXPO_PUBLIC_LOGROCKET_ENABLED=false
EXPO_PUBLIC_LOGROCKET_APP_ID=your-logrocket-app-id

# Environment
NODE_ENV=development
