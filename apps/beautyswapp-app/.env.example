# Medusa Backend Configuration
# Ces variables doivent être définies pour l'app mobile

# URL du backend Medusa (sans trailing slash)
EXPO_PUBLIC_MEDUSA_BACKEND_URL=http://localhost:9000

# Clé publique Medusa (obtenue depuis le dashboard Medusa)
EXPO_PUBLIC_MEDUSA_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxx

# Supabase Configuration
# Obtenir ces valeurs depuis https://supabase.com/dashboard/project/_/settings/api
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Environment
NODE_ENV=development
